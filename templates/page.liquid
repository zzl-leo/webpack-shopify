{% render 'shogun-products', content: page %}

<div class="page-width page-content">

  {% include 'breadcrumbs' %}

	<div class="grid">
    <div class="grid__item medium-up--three-quarters medium-up--push-one-eighth">

      <header class="section-header">
        {% if false %} {% include 'bss-title-store-locator' %} {% include 'bss-description-store-locator' %} {% else %} <h1 class="section-header__title">{{ page.title }}</h1> {% endif %}
      </header>

      <div class="rte rte--nomargin">
        {{ page.content }}
      </div>

    </div>
  </div>

</div>




