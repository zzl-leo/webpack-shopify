      {"type": "header", "content": "Website policy"},
			{"type": "checkbox", "id": "show_header_policy", "label": "Show an header policy", "default": true},
      {"type": "text", "id": "policy_a", "label": "Delivery", "default": "Free & Fast Delivery"},
			{"type": "text", "id": "policy_a_content", "label": "Delivery Content", "default": "We offer free and safe delivery on every order in the US. Delivery is usually within 3-7 business days via our carrier FedEx. (Due to COVID-19, delivery times may vary.)"},
      
			{"type": "text", "id": "policy_b", "label": "Guarantee", "default": "30-Day Money-Back Guarantee"},
			{"type": "text", "id": "policy_b_content", "label": "Guarantee Content", "default": "You can return any product for a full refund within 30 days of purchase. The refund will be made to your original payment method."},
      
			{"type": "text", "id": "policy_c", "label": "Warranty", "default": "2-Year Warranty"},
			{"type": "text", "id": "policy_c_content", "label": "Warranty Content", "default": "All purchases come with a free 2-year warranty. Purchases directly from our official website will receive a free automatic 1-year extension. (The Solar Generator 2000, Explorer 2000, SolarSaga 200  and Solar Panel Connector come with a 3-year warranty + 2-year free extension) "},
      
			{"type": "text", "id": "policy_d", "label": "Support", "default": "Lifetime Customer Support"},
			{"type": "text", "id": "policy_d_content", "label": "Support Content", "default": "Our dedicated customer service team will answer all your product questions within 24 hours (during business days). If you have any queries, feel free to email us at hello@jackery.com."},

			{"type": "text", "id": "policy_e", "label": "Payment", "default": "Outdoor Now, Pay Later"},
			{"type": "text", "id": "policy_e_content", "label": "Payment Content", "default": "Each order above $500 can be paid with easy monthly payments. Just select 'Affirm' at checkout to grab your Jackery."},

			{"type": "header", "content": "Announcement bar"},
      {"type": "checkbox", "id": "show_announcement", "label": "Show an announcement"},
      {
        "type": "text", "id": "announcement_text", "label": "Announcement text", "default": "Free shipping and returns",
        "info": "When closed, the announcement will remain closed until the next visit. Change the text to see it again."
      },
      {"type": "url", "id": "announcement_link", "label": "Announcement link"},
      {"type": "checkbox", "id": "announcement_closable", "label": "Allow users to close announcement"}
