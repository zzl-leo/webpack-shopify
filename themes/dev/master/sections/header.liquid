		<ul class="nav">
      {% for link in main_menu.links %}
        {% case link.title %}
          {% when 'Products' %}
          <li>
            <a href="##" class="nav-item">Products</a>
            
            <ul class="nav-child-item hidden-desktop mobile-nav-product">
              {% for childlink in link.links %}
              <li>
                <a href="##">{{ childlink.title }}</a>
                <ul class="child-style child-style-1">
                  {%- assign category = childlink.url | replace: '/collections/', '' -%}
                  {% for product in collections[category].products limit: 12 %}
				  {%- assign product_tags = product.tags | join: ',' -%}
                  <li>
                    <a href="{{ product.url }}" {%- if product.tags contains 'NEW' -%}class="new-product"{% endif %}><img src="{{ product.featured_image | img_url: '60x60' }}" alt="{{ product.title }}">{{ product.title }}</a>
                  </li>
                  {% endfor %}
                </ul>
              </li>
              {% endfor %}
            </ul>
            
            <div class="nav-collection hidden-phone">
              <div class="layer-container distance">
                <ul class="left-nav">
                  {% for childlink in link.links %}
					<li class="{% if forloop.index0 == 0 %}on{% endif %}">
						{%- assign href = '##' -%}
						{% if childlink.url != '/collections/accessories' %}
							{%- assign href = childlink.url | replace: 'collections', 'pages' -%}
						{% endif %}
						<a href="{{ href }}" class="nav-item">{{ childlink.title }}</a>
					</li>
				  {% endfor %}
                </ul>
                <figure class="right-product">
                  {% for childlink in link.links %}
                  <ul class="child-style child-style-1 {% if forloop.index0 == 0 %}on{% endif %}">
                    {%- assign category = childlink.url | replace: '/collections/', '' -%}
										<!-- category: {{ category }}, {{ childlink.url }} -->
                    {% for product in collections[category].products limit: 9 %}
                    <li
						dom-type="mkt"
						mkt-area="{{childlink.title}}-{{forloop.index}}"
						mkt-page="分类页"
						mkt-materialID="{{product.featured_image | img_url}}"
						mkt-materialname="{{product.variants[0].sku}}"
						mkt-skuprice="{{ product.price }}"
						mkt-commodity_tag="{{product_tags}}"
					>
                      <a href="{{ product.url }}" {%- if product.tags contains 'NEW' -%}class="new-product"{% endif %}><img src="{{ product.featured_image | img_url: '194x194' }}" alt="{{ product.title }}"><br />{{ product.title }}</a>
                    </li>
                    {% endfor %}
                    {% if collections[category].products_count > 9 %}
                    <li>
                      <a href="{{ childlink.url }}" class="viewAll flexCenter">See more</a>
                    </li>
                    {% endif %}
                  </ul>
                  {% endfor %}
                </figure>
              </div>
            </div>
          </li>
          {% else %}
          <li>
            <a href="{{ link.url }}#" class="nav-item">{{ link.title }}</a>
            {% if link.links != blank %}
            <ul class="nav-child-item">
              {% for childlink in link.links %}<li><a href="{{ childlink.url }}">{{ childlink.title }}</a></li>{% endfor %}
            </ul>
            {% endif %}
          </li>
        {% endcase %}
      {% endfor %}
		</ul>
		
