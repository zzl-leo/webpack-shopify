        <img class="gift-shadow" src="https://cdn.shopify.com/s/files/1/0550/0524/9633/files/d55cafef11cd47810614c6b2b120893c.png?v=1663834930" alt="">
      </div>
      <div class="text-loading">
        {{ 'tenth.text_loading' | t }}
        <div class="loading">
          <div></div>
          <div></div>
          <div></div>
        </div>
      </div>
    </div>
    <div class="tenth-three-step J-tenth-three display-none">
      <div class="title">{{ 'tenth.success_title' | t }}</div>
      <div class="subtitle" data-success-text="{{ 'tenth.success_subtitle' | t }}" data-register-text="{{ 'tenth.register_subtitle' | t }}">{{ 'tenth.success_subtitle' | t }}</div>
      <div class="tenth-prizes-img J-tenth-prizes-img">
        <img src="" alt="">
      </div>
      <div class="coupon-text J-tenth-coupon-text display-none">
        <div class="coupon-title"><span class="item-icon"></span>{{ 'tenth.order_over' | t }} <span class="J-tenth-limit">$2000</span></div>
        <div class="coupon-item"><span class="item-icon"></span><span>{{ 'tenth.valid_time' | t }}: </span><span class="J-tenth-time"></span></div>
        <div class="coupon-item"><span class="item-icon"></span>{{ 'tenth.coupons_tip' | t }}</div>
      </div>
      <div class="coupon-text J-tenth-500-text display-none">
        <div class="coupon-item"><span class="item-icon"></span><span>{{ 'tenth.coupon_500' | t }}</span></div>
      </div>
      <div class="option-button">
        <div class="J-tenth-stroll" data-success-text="{{ 'tenth.another_stroll' | t }}" data-register-text="{{ 'tenth.register_later' | t }}">{{ 'tenth.another_stroll' | t }}</div>
        <div class="J-tenth-prizes J-prize-text" data-success-text="{{ 'tenth.redeem_prizes' | t }}" data-register-text="{{ 'tenth.register_now' | t }}">{{ 'tenth.redeem_prizes' | t }}</div>
      </div>
      <div class="code-tip J-code-tip">
        <div>
          {{ 'tenth.coupons_code' | t }}
          <span class="tenth-code">
            <span class="J-tenth-code" dom-type="coupon"></span>
            <span class="copy-tip J-tenth-code-tip">copy success!</span>
          </span>
        </div>
        <div>{{ 'tenth.checkout_page' | t }}</div>
        <div>{{ 'tenth.send_email' | t }}</div>
      </div>
      <div class="register-tip J-register-tip display-none">
        <div>{{ 'tenth.register_tip1' | t }}</div>
        <div>{{ 'tenth.register_tip2' | t }}</div>
        <div>{{ 'tenth.register_tip3' | t }}</div>
      </div>
    </div>
  </div>
</div>

