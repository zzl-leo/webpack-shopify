<div class="tenth-button J-tenth-button J-tenth-button-hover">
  <img src="https://cdn.shopify.com/s/files/1/0550/0524/9633/files/11_4.svg?v=1665366252" alt="">
</div>

<div id="tenth-pop" class="tenth-pop-wrap">
  <div class="tenth-box">
    <div class="close-btn J-tenth-close" tenth-current-step="one">
      <img src="https://cdn.shopify.com/s/files/1/0550/0524/9633/files/X.png?v=1663318075" alt="">
    </div>
    <div class="tenth-one-step J-tenth-one">
      <div class="title">{{ 'tenth.title' | t }}</div>
      <div class="tenth-icon J-tenth-icon">
        <div class="tag"><span>100%</span> {{ 'tenth.tag_one' | t }}</div>
        <div class="tag">{{ 'tenth.tag_two' | t }} <span>$2099</span></div>
      </div>
      <div class="gift-box">
        <img class="pulsate-fwd J-tenth-submit" src="https://cdn.shopify.com/s/files/1/0550/0524/9633/files/Open_2.png?v=1663898157" alt="">
      </div>
      <div class="form-wrap J-tenth-form">
        <input type="email" id="tenthEmail" class="input-full J-tenth-email" name="tenthEmail" autocorrect="off" autocapitalize="off" placeholder="{{ 'tenth.placeholder' | t }}">
        <div class="submit J-tenth-submit">{{ 'tenth.submit' | t }}</div>
      </div>
      <div class="error-tip J-tenth-tip" data-email-tip="{{ 'tenth.email_tip' | t }}" data-emailerror-tip="{{ 'tenth.email_error_tip' | t }}"></div>
      <div class="declaration">
        <input type="checkbox" class="checkbox J-tenth-checkbox" />
        <span>
          {{ 'tenth.declaration' | t }}
          <a href="https://www.jackery.com/policies/terms-of-service">{{ 'subscribe.terms_of_ues' | t }}</a> {{ 'subscribe.and' | t }} <a href="https://www.jackery.com/policies/privacy-policy">{{ 'subscribe.privacy_policy' | t }}</a>
        </span>
      </div>
    </div>
    <div class="tenth-has-join J-tenth-joined display-none">
      <div class="title">{{ 'tenth.joined_title' | t }}</div>
      <div class="awards-show">
        <img class="J-tenth-product-url" src="https://cdn.shopify.com/s/files/1/0550/0524/9633/files/banner_1.png?v=1660892591" alt="">
      </div>
      <div class="coupon-text J-tenth-coupon-text display-none">
        <div class="coupon-title"><span class="item-icon"></span>{{ 'tenth.order_over' | t }} <span class="J-tenth-limit">$2000</span></div>
        <div class="coupon-item"><span class="item-icon"></span><span>{{ 'tenth.valid_time' | t }}: </span><span class="J-tenth-time"></span></div>
        <div class="coupon-item"><span class="item-icon"></span>{{ 'tenth.coupons_tip' | t }}</div>
      </div>
      <!-- <div class="subtitle">{{ 'tenth.joined_subtitle' | t }}</div> -->
      <div class="code-tip J-code-tip">
        <div>
          <span>{{ 'tenth.joined_subtitle' | t }}</span>
          <span class="has-join-code">
            {{ 'tenth.joined_coupons_code' | t }}
            <span class="tenth-code">
              <span class="J-tenth-code" dom-type="coupon"></span>
              <span class="copy-tip J-tenth-code-tip">copy success!</span>
            </span>
            <br/>
            <span class="J-tenth-joinedText" data-joined-product="{{ 'tenth.joined_send_email_product' | t }}" data-joined-coupon="{{ 'tenth.joined_send_email_coupon' | t }}">{{ 'tenth.joined_send_email_product' | t }}</span>
          </span>
        </div>
      </div>
    </div>
    <div class="tenth-two-step J-tenth-two display-none">
      <!-- <div class="title">{{ 'tenth.title' | t }}</div> -->
      <div class="tenth-gift J-tenth-gift">
        <img class="lid" src="https://cdn.shopify.com/s/files/1/0550/0524/9633/files/ff56b8ad38e29ce40038bc757cb9aaae.png?v=1663834923" alt="">
