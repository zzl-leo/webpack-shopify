    if (count_down) { //显示倒计时
        html = html + timer_html;
    }
    if (is_jp) { //按优惠类型显示，日本样式修改
        html = html.replace(/円/g, "");
    }
    
    var $coupon = $("<div id='coupon-container'></div>");
    $coupon.html(html);
    var $coupon_item = $coupon.find(".coupon-style");
    $coupon_item.on("click", function () {
        copy(coupon_code, is_jp ? "クーポンコードをコピーしました。" : "Copy succeeded");
    });

    // inset to hr after
    $(".hr--medium").eq(0).after($coupon);

    if (count_down) { //显示倒计时
        var now = parseInt(server_time, 10) * 1000;
        leftTimefn(time_begin, time_end, now, now);
    }

    /**********cdp-优惠券**********/
    document.querySelector("#coupon-container").setAttribute("dom-type", "coupon")
    const coupon_info = {
        coupon_code: data.coupon_code,
    }

    if(data.type === 1) { // 金额
        coupon_info.coupon_amount = Number(data.discount) || 0
    } else { // 百分比优惠
        coupon_info.coupon_discount = (Number(data.discount) || 0) / 100
    }
    document.querySelector("#coupon-container").setAttribute("coupon-data", JSON.stringify(coupon_info))
    CouponExposure(coupon_info)
    ReceiveCoupon()
    /**********cdp-优惠券**********/
}

function isJp(style) {
    var jp_styles = "4-A,4-B,4-C";
    return jp_styles.indexOf(style) >= 0;
}
