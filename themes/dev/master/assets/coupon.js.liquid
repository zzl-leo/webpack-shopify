function copy(value, msg) {
    const input = document.createElement("input");
    input.setAttribute("readonly", "readonly");
    input.setAttribute("value", value);
    document.body.appendChild(input);
    input.select();
    input.setSelectionRange(0, 9999);
    if (document.execCommand("copy")) {
        document.execCommand("copy");
        $("#coupon-success").html(msg).show();
        // toast(msg, 2000, "rose");
        setTimeout(function () {
            $("#coupon-success").hide();
        }, 2000);
    }
    document.body.removeChild(input);
}

function leftTimefn(begin_time, end_time, now, start) {
    var dotHtml;
    var endTime = new Date(end_time); //结束时间
    var curTime = new Date(begin_time); //当前时间
    var leftTime = parseInt((endTime.getTime() - curTime.getTime()) / 1000); //获得时间差
    //小时、分、秒需要取模运算
    var d = parseInt(leftTime / (60 * 60 * 24)) + "";
    var h = parseInt(leftTime / (60 * 60) % 24) + "";
    var m = parseInt(leftTime / 60 % 60) + "";
    var s = parseInt(leftTime % 60) + "";
    if (leftTime <= 0) {
        clearTimeout(set);
        $(".coupon-timer").remove();
        return;
    }
    dotHtml = `<span class="dot">` + (d[d.length - 2] ? d[d.length - 2] : `0`) + `</span><span class="dot">` + d[d.length - 1] + `</span><span class="dot1">:</span><span class="dot">` + (h[h.length - 2] ? h[h.length - 2] : `0`) + `</span><span class="dot">` + h[h.length - 1] + `</span><span class="dot1">:</span><span class="dot">` + (m[m.length - 2] ? m[m.length - 2] : `0`) + `</span><span class="dot">` + m[m.length - 1] + `</span><span class="dot1">:</span><span class="dot">` + (s[s.length - 2] ? s[s.length - 2] : `0`) + `</span><span class="dot">` + s[s.length - 1] + `</span>`;
    $(".coupon-timer").html(dotHtml);
    setTimeout(function () {
        var cur = new Date().getTime();
        begin_time = new Date(start).getTime() + (cur - now);
        leftTimefn(begin_time, end_time, now, start);
    }, 1000)
};

function getCouponInfo (id) {
    var variant = meta.product.variants.find(item => item.id === +id);
    var sku_id = variant.id;
    var price = variant.price;
    price && (price = parseFloat(price / 100).toFixed(2));

    $api.getCouponInfo({ sku_id, price }, function(r) {
        document.querySelector("#coupon-container") && document.querySelector("#coupon-container").remove();
        renderCoupon(r)
    }, function(err) {
        console.error(err)
    })
}
